<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false">

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %msg%n</Pattern>
		</encoder>
	</appender>
	
	<root>
		<level value="warn" />
		<appender-ref ref="STDOUT" />
	</root>
</configuration>

